<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core"
	xmlns:u="sap.ui.unified">
	<Dialog contentHeight="100px" contentWidth="200px" title="Import Spreadsheet">
		<u:FileUploader id="fileUploader" name="myFileUpload" uploadUrl="upload/" fileType="{CreateModel>/fileTypes}"
	class="sapUiSmallMarginBegin"		uploadComplete="handleUploadComplete"/>
		<beginButton>
			<Button press="onUpdateRecord" text="Import"></Button>
		</beginButton>
		<endButton>
			<Button text="Cancel" press="onCloseSpreadsheet"></Button>
		</endButton>
	</Dialog>
</core:FragmentDefinition>